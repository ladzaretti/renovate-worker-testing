{
  "baseBranches": ["dev", "main"],
  "dependencyDashboard": "true",
  "packageRules": [{
    "groupName": "prod",
    "matchPaths": ["prod/**/sub3"],
    "addLabels": ["prod"]
  },
    {
      "groupName": "dev",
      "matchPaths": ["dev/**/sub3"],
      "addLabels": ["dev"]
    },{
      "matchManagers": ["nodenv"],
      "ignoreUnstable": "false"
    }
  ]
}
